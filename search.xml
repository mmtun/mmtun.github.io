PartLine(E40&lt;Ref(E40,3) Or E60&lt;Ref(E60,3),E40),ColorGreen;
PartLine(E45&lt;Ref(E45,3) Or E60&lt;Ref(E60,3),E45),ColorGreen;
PartLine(E50&lt;Ref(E50,3) Or E60&lt;Ref(E60,3),E50),ColorGreen;
